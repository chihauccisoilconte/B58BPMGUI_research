; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="tesi"
run/main_scene="res://scenes/Bootsplash.tscn"
boot_splash/image="res://assets/tesi_interfaccia0.png"
config/icon="res://icon.png"

[autoload]

DataManager="*res://scripts/dataManager.gd"
Global="*res://scripts/global.gd"
SaveArchive="*res://scripts/save_archive.gd"

[display]

window/size/width=1080
window/size/height=1920
window/size/test_width=1080
window/size/test_height=2220
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep_width"

[gui]

common/drop_mouse_on_gui_input_disabled=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_clear_color=Color( 0.176471, 0.176471, 0.176471, 1 )
environment/default_environment="res://default_env.tres"
